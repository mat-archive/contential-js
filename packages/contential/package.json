{
  "name": "contential",
  "version": "0.0.23",
  "license": "MIT",
  "private": false,
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./chat": {
      "default": "./dist/chat/index.js",
      "import": "./dist/chat/index.mjs",
      "require": "./dist/chat/index.js",
      "types": "./dist/chat/index.d.ts"
    },
    "./prompt": {
      "default": "./dist/prompt/index.js",
      "import": "./dist/prompt/index.mjs",
      "require": "./dist/prompt/index.js",
      "types": "./dist/prompt/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rimraf dist",
    "build": "npm-run-all clean build:main",
    "build:main": "tsup",
    "lint": "eslint \"**/*.ts*\"",
    "publish:prod": "changeset version && changeset publish"
  },
  "dependencies": {
    "@contential/chat": "0.0.6",
    "@contential/prompt": "0.0.14"
  }
}
