{
  "name": "contential-js",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nx run-many --target=dev --all --parallel=20",
    "build": "nx run-many --target=build --all",
    "affected": "nx show projects --affected",
    "affected:build": "nx affected:build",
    "publish:prod1": "nx affected --base=origin/production~1 --target=publish:prod --exclude=apps/* --verbose",
    "changeset": "changeset",
    "release": "changeset version && changeset publish"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "npm-run-all": "^4.1.5",
    "nx": "16.5.1",
    "rimraf": "^5.0.1",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
